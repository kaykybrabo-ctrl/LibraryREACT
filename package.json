{
  "name": "projecttype",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"npm run react-dev\" \"npm run server\"",
    "server": "ts-node src/server.ts",
    "build": "npm run react-build && npm run server-build",
    "server-build": "tsc --build tsconfig.backend.json",
    "react-dev": "vite",
    "react-build": "vite build",
    "preview": "vite preview",
    "seed": "ts-node src/scripts/seed.ts",
    "start": "node dist/server.js",
    "docker:build": "./docker-rebuild.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "clean": "rm -rf node_modules package-lock.json dist FRONTEND/react-dist",
    "fresh-install": "npm run clean && npm install"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.1",
    "@types/bcrypt": "^6.0.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^5.0.1",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "jsonwebtoken": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "multer-storage-cloudinary": "^4.0.0",
    "mysql2": "^3.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "react-toastify": "^11.0.5",
    "vite": "^7.1.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.23",
    "concurrently": "^9.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
